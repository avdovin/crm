<tr onClick="javascript:document.location.href='<%= edit_crm_domain_path(domain) %>'" class="yellow items" data-item-id="<%= domain.id %>">
  <td><input type="checkbox" class="chb_selectItem" value="<%= domain.id %>"/></td>
  <td><%= domain.name %></td>
  <td><%= domain.regdate ? domain.regdate.strftime("%d.%m.%Y") : '-' %></td>
  <td><%= domain.paiddate ? domain.paiddate.strftime("%d.%m.%Y") : '-' %></td>
  <td><a class="action" href="<%= edit_crm_domain_path(domain) %>" title="Смотреть"><img src="/crm/img/edit.png" width="16" height="16" alt=""></a></td>
</tr>

